<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.heba.testproject.DoctorGraphActivity">
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/saveGraphBtn"
        android:text="Save"
        android:onClick="saveGraph"
        />
    <com.github.mikephil.charting.charts.BarChart
        android:id="@+id/bargraph"
        android:layout_below="@id/saveGraphBtn"
        android:layout_width="match_parent"
        android:layout_height="400dp"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"></com.github.mikephil.charting.charts.BarChart>
</RelativeLayout>
