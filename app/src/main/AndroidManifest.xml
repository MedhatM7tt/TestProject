<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.heba.testproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:label="Project"
            android:configChanges="keyboard|orientation|screenSize"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ChooseActivity"
            android:label="Choosing"
            android:configChanges="keyboard|orientation|screenSize" />
        <activity android:name=".StudentLoginActivity"
            android:label="Student"
            android:configChanges="keyboard|orientation|screenSize" />
        <activity android:name=".DocLoginActivity"
            android:label="Doctor"
            android:configChanges="keyboard|orientation|screenSize"/>
        <activity android:name=".RegActivity"
            android:label="Reg"
            android:configChanges="keyboard|orientation|screenSize"></activity>
    </application>

</manifest>